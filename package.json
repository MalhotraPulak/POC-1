{
  "name": "RnProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start": "react-native esbuild-start",
    "metro": "react-native start",
    "metro-android": "react-native run-android", 
